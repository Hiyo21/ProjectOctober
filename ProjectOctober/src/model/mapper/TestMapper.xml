<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="model.mapper.TestMapper">

	<insert id="insertComponent" parameterType="Test">
		insert into component values (#{id}, #{x}, #{y}, #{width}, #{height}) 
	</insert>
	
	<select id="getComponentList" resultType="Test">
		select * from component
	</select>

</mapper>